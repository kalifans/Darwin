echo "deb https://coolbooter.com/ ./" >> /etc/apt/sources.list.d/cydia.list
apt-get update
apt-get install com.coolbooter.coolbootercli
or
wget --no-check-certificate https://github.com/kalifans/Darwin/raw/deb/coolbootercli_0.6.3-release_iphoneos-arm.deb
dpkg -i com.coolbooter.coolbootercli_0.6.3-release_iphoneos-arm.deb
apt-get install -f
