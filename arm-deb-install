# tihmstar Cydia 源
echo "deb http://repo.tihmstar.net/ ./" >> /etc/apt/sources.list.d/net.tihmstar.list
# angelxwind Cydia 源
echo "deb http://cydia.angelxwind.net/ ./" >> /etc/apt/sources.list.d/net.angelxwind.list

更新
apt-get update
apt-get upgrade

# 清除垃圾
apt-get autoremove
apt-get autoclean
apt-get clean all

# Appsync Unified iOS 8.4.1
apt-get install net.angelxwind.appsyncunified

# Apple File Conduit "2" iOS 8.4.1
apt-get install com.saurik.afc2d

# GDB
echo "deb http://cydia.radare.org/ ./" >> /etc/apt/sources.list.d/org.radare.list
apt-get update
apt-get install adv-cmds gdb

# Terminal
wget --no-check-certificate https://github.com/kalifans/Darwin/raw/IOS/iosterminal_1.0.2-3_ios6.deb
wget --no-check-certificate https://github.com/kalifans/Darwin/raw/IOS/iosterminal_1.0.2-3_ios7.deb
wget --no-check-certificate https://github.com/kalifans/Darwin/raw/IOS/iosterminal_1.0.2-4_ios9.deb

# iFile iOS 6
